<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hasee.virtualwinos">

    <uses-permission android:name="android.permission.BODY_SENSORS" />
    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@drawable/os_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/os_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        <activity
            android:name=".System_Control.system_state"
            android:configChanges="orientation|keyboard"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activitys.login"
            android:configChanges="orientation|keyboard"
            android:screenOrientation="landscape" />
        <activity
            android:name=".Activitys.Desktop"
            android:configChanges="orientation|keyboard"
            android:screenOrientation="landscape" />
        <activity
            android:name=".Activitys.lock"
            android:configChanges="orientation|keyboard"
            android:screenOrientation="landscape" />
        <activity
            android:name=".Activitys.startAnimation"
            android:configChanges="orientation|keyboard"
            android:screenOrientation="landscape" />
        <activity
            android:name=".Activitys.first_open"
            android:configChanges="orientation|keyboard"
            android:screenOrientation="landscape" />
        <activity android:name=".Activitys.showwallpaper"
            android:configChanges="orientation|keyboard"
            android:screenOrientation="landscape"></activity>
    </application>

</manifest>